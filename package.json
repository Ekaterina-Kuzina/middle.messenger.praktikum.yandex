{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.16",
    "npm": ">=9.5"
  },
  "scripts": {
    "dev": "vite",
    "prebuild": "tsc --noEmit",
    "build": "vite build",
    "start": "vite build && node server.js",
    "preview": "vite preview",
    "test": "mocha --es-module-specifier-resolution=node",
    "prepare": "husky install",
    "lint:ts": "eslint . --ext .ts,.js",
    "lint:css": "stylelint \"**/*.scss\"",
    "pre-commit": "npm run lint:ts && npm run lint:css && npm run test"
  },
  "devDependencies": {
    "@types/node": "^20.12.8",
    "@types/chai": "^4.3.16",
    "@types/jsdom": "^21.1.7",
    "@types/mocha": "^10.0.6",
    "@types/sinon": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "express": "^4.18.3",
    "prettier": "^3.2.5",
    "sass": "^1.71.1",
    "stylelint": "^16.5.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vite": "^5.1.4",
    "vite-plugin-copy": "^0.1.6",
    "vite-plugin-style-import": "^2.0.0",
    "jsdom": "^24.0.0",
    "husky": "^8.0.3",
    "mocha": "^10.5.2",
    "sinon": "^18.0.0",
    "chai": "^5.1.1"
  },
  "dependencies": {
    "handlebars": "^4.7.8",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-scss": "^6.2.1"
  }
}
